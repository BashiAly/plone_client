    "prestart": "npm run build",
    "server": "nodemon dist/server/bundle.js",
    "start": "npm run server",
    "predebug": "npm run build",
    "debug": "node --debug-brk dist/server/bundle.js",
    "test": "karma start",
    "tslint": "tslint",
    "lint": "npm run tslint './src/**/*.ts'",
    "pretest": "npm run lint"
  },
